# Snowdrop editorconfig file
# More info is available here : http://EditorConfig.org)

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,xml,yaml,yml,json}]
charset = utf-8
indent_style = space
indent_size = 2

# 4 space indentation
[*.java]
indent_style = space
indent_size = 4
continuation_indent_size = 8